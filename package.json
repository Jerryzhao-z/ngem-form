{
  "name": "ngem-form",
  "version": "1.0.0",
  "description": "a form module for easy use",
  "main": "dist/ngemForm.js",
  "scripts": {
    "build:dist": "webpack --progress --colors --set-env-NODE_ENV=development",
    "build:prod": "webpack --progress --colors --set-env-NODE_ENV=production",
    "build": "npm run build:dist & npm run build:prod",
    "eslint:test": "eslint -c other/test.eslintrc --ignore-pattern **/*.{test,mock}.js src/",
    "eslint:src": "eslint -c other/src.eslintrc --ignore-pattern !**/*.{test,mock}.js src/",
    "eslint": "npm run eslint:test -s && npm run eslint:src -s",
    "test": "karma start --single-run --set-env-COVERAGE=true --set-env-NODE_ENV=test",
    "test:watch": "karma start --set-env-COVERAGE=true --set-env-NODE_ENV=test",
    "test:debug": "karma start --browsers Chrome --set-env-NODE_ENV=test",
    "start": "npm run test:watch",
    "check-coverage": "istanbul check-coverage --statements 93 --branches 89 --functions 92 --lines 92",
    "report-coverage": "cat ./coverage/lcov.info | node_modules/.bin/codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jerryzhao-z/ngem-form.git"
  },
  "keywords": [
    "webpack",
    "ES6",
    "ES2015",
    "Angular1.x",
    "form"
  ],
  "author": "Jerryzhao-z",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.5.7",
    "angular-ui-router": "^0.3.1",
    "bootstrap": "^3.3.6",
    "jquery": "^3.1.0",
    "http-server": "^0.9.0",
    "md5": "^2.1.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.5.7",
    "argv-set-env": "^1.0.1",
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.23.1",
    "deindent": "^0.1.0",
    "eslint": "^3.0.1",
    "eslint-loader": "^1.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "isparta": "^4.0.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^0.4.4",
    "jasmine": "^2.4.1",
    "jasmine-core": "^2.4.1",
    "json-loader": "^0.5.4",
    "karma": "^1.1.2",
    "karma-chrome-launcher": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.0.0",
    "karma-jasmine": "^1.0.2",
    "karma-webpack": "^1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "lodash": "^4.14.1",
    "ng-annotate": "^1.2.1",
    "ng-annotate-loader": "^0.1.1",
    "path-here": "^1.1.1",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.3",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-notifier": "^1.3.2"
  }
}
